<?xml version="1.0" encoding="UTF-8"?>
<configuration status="OFF">
	<appenders>
		<Console name="myConsole" target="SYSTEM_OUT">
			<PatternLayout pattern="[%-5p] %m%n" />
		</Console>
	</appenders>
	<loggers>
		<root level="info">
			<appender-ref ref="myConsole"/>
		</root>
	</loggers>
</configuration>


































